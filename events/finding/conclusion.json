{
  "name": "Conclusion",
  "category": "finding",
  "type": "conclusion",
  "extends": "base_event",
  "description": "The conclusion events report the results of detections or analytics.",
  "attributes": {
    "disposition": {
      "group": "classification"
    },
    "disposition_id": {
      "$include": "enums/defaults.json",
      "group": "classification",
      "requirement": "required"
    },
    "end_time": {
      "requirement": "recommended"
    },
    "message": {
      "description": "The description of the finding or incident.",
      "group": "primary"
    },
    "priority": {
      "group": "primary"
    },
    "priority_id": {
      "description": "The normalized priority.</p>Priority identifies the relative importance of the finding. It is a measurement of urgency.",
      "group": "primary",
      "requirement": "recommended",
      "$include": "enums/finding_priority.json"
    },
    "rule": {
      "group": "primary",
      "requirement": "required"
    },
    "start_time": {
      "requirement": "recommended"
    }
  }
}
