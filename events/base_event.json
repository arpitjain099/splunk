{
  "caption": "Extended Base Event",
  "description": "The extended base event adds the inferred caller and target objects.",
  "extends": "base_event",
  "profiles": [
    "splunk/ba"
  ],
  "attributes": {
    "$include": [
      "profiles/ba.json"
    ],
    "caller": {
      "group": "context",
      "profile": "splunk/ba",
      "requirement": "optional"
    },
    "target": {
      "group": "context",
      "profile": "splunk/ba",
      "requirement": "optional"
    }
  }
}